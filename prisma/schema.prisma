generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model  FormData {
  id    Int     @id @default(autoincrement())
  companyName String
  createdAt DateTime @default(now())
  email String?
  submittedAt DateTime?
  projectName String?
  projectAddress String?
  updatedAt DateTime @default(now())
}
