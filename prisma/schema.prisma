generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model  FormData {
  id    Int     @id @default(autoincrement())
  title String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  email String  @unique
  submittedAt DateTime?
  companyName String
  projectName String
  projectAddress String
}
